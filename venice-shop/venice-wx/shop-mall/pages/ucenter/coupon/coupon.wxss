page {
  background: #f4f4f4;
  min-height: 100%;
}

.ell {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}

.clearfix {
  zoom: 1;
}

.container {
  background: #f4f4f4;
  min-height: 100%;
}

.coupon-form {
  height: 110rpx;
  width: 100%;
  background: #fff;
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 20rpx;
  display: flex;
}

.input-box {
  flex: 1;
  height: 70rpx;
  color: #333;
  font-size: 24rpx;
  background: #fff;
  position: relative;
  border: 1px solid rgba(0, 0, 0, 0.15);
  border-radius: 4rpx;
  margin-right: 30rpx;
}

.input-box .coupon-sn {
  position: absolute;
  top: 10rpx;
  left: 30rpx;
  height: 50rpx;
  width: 100%;
  color: #000;
  line-height: 50rpx;
  font-size: 24rpx;
}

.clear-icon {
  position: absolute;
  top: 21rpx;
  right: 30rpx;
  width: 28rpx;
  height: 28rpx;
}

.add-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 70rpx;
  border: none;
  width: 168rpx;
  background: #b4282d;
  border-radius: 0;
  line-height: 70rpx;
  color: #fff;
  font-size: 28rpx;
}

.add-btn.disabled {
  background: #ccc;
}

.help {
  height: 72rpx;
  line-height: 72rpx;
  text-align: right;
  padding-right: 30rpx;
  background: url(https://platform-wxmall.oss-cn-beijing.aliyuncs.com/upload/20180727/15032866437ca8.png) 590rpx center no-repeat;
  background-size: 28rpx;
  color: #999;
  font-size: 24rpx;
}

.coupon-list {
  width: 100%;
  height: auto;
  overflow: hidden;
  padding-left: 30rpx;
  padding-right: 30rpx;
}

.getCoupon {
  width: 100%;
  position: relative;
}

.coupon-img {
  width: 100%;
  height: 249rpx;
  margin: o auto;
  display: block;
}

.coupon-nav {
  display: flex;
  height: 70rpx;
  width: 100%;
  margin-top: 20rpx;
  background-color: #fff;
}

.nav-item {
  flex: 1;
  height: 70rpx;
  line-height: 70rpx;
  border-bottom: 5rpx solid #fff;
  text-align: center;
  font-size: 24rpx;
}

.nav-item.on {
  border-bottom: 5rpx solid #f7a700;
  color: #f7a700;
}

.coupon-item {
  padding: 25rpx 10rpx 0;
}

.coupon-item .item {
  width: 100%;
  position: relative;
}

.coupon-item .item .img-left {
  width: 63rpx;
  height: 208rpx;
  position: absolute;
  display: block;
  left: 0;
  top: 0;
}

.coupon-item .item .content {
  margin-left: 67rpx;
  height: 208rpx;
  background-color: #fff;
  border-radius: 10rpx;
  overflow: hidden;
}

.coupon-item .item .content-item {
  padding: 25rpx 30rpx 0;
  position: relative;
}

.coupon-item .item .content-item .time {
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 18rpx;
  color: #9b9b9b;
  text-align: left;
  border-top: 2rpx dotted #e9e9e9;
}

.coupon-item .item .content-item .jt {
  color: #9b9b9b;
  font-size: 14rpx;
  position: absolute;
  top: 90rpx;
  right: 20rpx;
  width: 16rpx;
  height: 26rpx;
}

.coupon-item .item .content-item .item-1 {
  width: 100%;
  height: 130rpx;
}

.coupon-item .item .content-item .price {
  color: #21bece;
  font-size: 82rpx;
  font-weight: bold;
  height: 130rpx;
  width: 48%;
  float: left;
}

.coupon-item .item .content-item .price text {
  font-size: 34rpx;
  color: #21bece;
  font-weight: bold;
}

.coupon-item .item .content-item .name-adr {
  float: left;
  width: 48%;
  height: 130rpx;
}

.coupon-item .item .content-item .name {
  font-size: 30rpx;
  color: #424242;
  text-align: left;
  margin-top: 20rpx;
}

.coupon-item .item .content-item .adr {
  font-size: 24rpx;
  color: #9b9b9b;
  text-align: left;
  margin-top: 10rpx;
}

.loadmore {
  height: 100rpx;
  width: 100%;
  line-height: 80rpx;
  text-align: center;
  margin-top: 0rpx;
}

.loadmore text {
  color: #999;
}

@keyframes loading {
  0% {
    transform: rotate(0deg);
  }

  50% {
    transform: rotate(180deg);
  }

  100% {
    transform: rotate(360deg);
  }
}

.loading {
  display: inline-block;
  transform-origin: 50% 50%;
  animation: loading 1s linear infinite;
}

@font-face {
  font-family: "iconfont";
  src: url('iconfont.eot?t=1520838078027'); /* IE9*/
  src: url('iconfont.eot?t=1520838078027#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
         url('iconfont.ttf?t=1520838078027') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+*/ url('iconfont.svg?t=1520838078027#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-release-up:before {
  content: "\e987";
}

.icon-complete:before {
  content: "\e992";
}

.icon-pull-down:before {
  content: "\e996";
}

.icon-loading:before {
  content: "\e9ac";
}

.gray {
  -webkit-filter: grayscale(100%);
  -moz-filter: grayscale(100%);
  -ms-filter: grayscale(100%);
  -o-filter: grayscale(100%);
  filter: grayscale(100%);
  filter: gray;
}
